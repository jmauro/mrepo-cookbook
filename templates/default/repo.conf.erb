[<%= @name %>]
<% if ! @arch.nil? -%>
name = <%= @description %>
<%end -%>
<% if ! @release.nil? -%>
release = <%= @release %>
<% end -%>
<% if ! @arch.nil? -%>
arch = <%= @arch %>
<% end -%>
<% if ! @metadata.nil? -%>
metadata = <%= @metadata %>
<% end -%>
<% if ! @iso.nil? -%>
iso = <%= @iso %>
<% end -%>
<% if ! @urls.nil? -%>
<% @urls.keys.sort.each do |name| -%>
<%= name %> = <%= @urls[name] %>
<% end -%>
<% end -%>
